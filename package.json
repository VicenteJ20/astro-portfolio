{
  "name": "astro-portfolio",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "tailwind-public-project": "tailwindcss -i src/styles/projectStyle.css -o public/styles/projectStyle.css",
    "tailwind-public-post": "tailwindcss -i src/styles/postStyle.css -o public/styles/postStyle.css",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && tailwindcss -i src/styles/projectStyle.css -o public/styles/projectStyle.css && tailwindcss -i src/styles/postStyle.css -o public/styles/postStyle.css && astro build",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/check": "^0.7.0",
    "@astrojs/rss": "^4.0.6",
    "@astrojs/sitemap": "^3.1.6",
    "@astrojs/tailwind": "^5.1.0",
    "@fontsource/poppins": "^5.0.14",
    "@fontsource/twinkle-star": "^5.0.20",
    "astro": "^4.10.1",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.4.5"
  }
}